{
  "name": "embrace",
  "version": "0.0.1",
  "description": "",
  "browserslist": "defaults",
  "source": "./index.html",
  "targets": {
    "default": {
      "distDir": "./output"
    }
  },
  "keywords": [
    "scss",
    "sass",
    "responsive",
    "html",
    "css",
    "mobilefirst",
    "sprucecss"
  ],
  "license": "",
  "author": "Jamal Mohamed Ameer",
  "scripts": {
    "sass:prod": "sass --no-source-map --style=compressed scss:css",
    "sass:dev": "sass scss:css",
    "autoprefixer": "postcss css/*.css --use autoprefixer -d minifycss/",
    "build": "parcel build -d docs/ --public-url ./ index.html",
    "dev": "parcel index.html",
    "start": "npm run dev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "parcel-bundler": "^1.8.1",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "sass": "^1.58.1"
  },
  "dependencies": {
    "@drewbot/sass-flexbox-grid": "^2.0.7",
    "@fortawesome/fontawesome-free": "^6.3.0",
    "@splidejs/splide": "^4.1.4",
    "aos": "^2.3.4",
    "bootstrap": "^5.2.3",
    "feather-icons": "^4.29.0",
    "sprucecss": "^1.2.1"
  }

}