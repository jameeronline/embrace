{
  "name": "embrace",
  "version": "0.0.1",
  "description": "",
  "keywords": [
    "scss",
    "sass",
    "responsive",
    "html",
    "css",
    "mobilefirst",
    "sprucecss"
  ],
  "license": "",
  "author": "Jamal Mohamed Ameer",
  "scripts": {
    "sass:prod": "sass --load-path=node_modules --no-source-map --style=compressed scss:css",
    "sass:dev": "sass --load-path=node_modules scss:css",
    "autoprefixer:dev": "postcss css/index.css --use autoprefixer -o css/index.css",
    "build": "npm run sass:prod && npm run autoprefixer:dev && parcel build --public-url ./ index.html",
    "dev": "parcel index.html --open",
    "start": "npm run build && npm run dev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "parcel-bundler": "^1.8.1",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "sass": "^1.58.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.3.0",
    "@splidejs/splide": "^4.1.4",
    "aos": "^2.3.4",
    "sprucecss": "^1.2.1"
  }
}
